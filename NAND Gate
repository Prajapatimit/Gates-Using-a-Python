#NAND Gate
def NAND (a, b):
    if a == 1 and b == 1:
        return False
    else:
        return True
 
if __name__=='__main__':
    print(NAND(1, 0))
    print(" A = 0, B = 0| A NAND B =",NAND(0,0)," | ")
    print(" A =0, B = 1 | A NAND B =",NAND(0,1)," | ")
    print(" A = 1, B =0 | A NAND B =",NAND(1,0)," | ")
    print(" A = 1, B =1 | A NAND B =",NAND(1,1)," | ")

